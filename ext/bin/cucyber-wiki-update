#!/bin/sh -e
git -C /var/lib/git/cucyber/website fetch --all && git -C /var/lib/git/cucyber/website reset --hard origin/dev
git -C /var/lib/git/cucyber/wiki fetch --all && git -C /var/lib/git/cucyber/wiki reset --hard origin/master
make -C /var/lib/git/cucyber/wiki clean
make -C /var/lib/git/cucyber/wiki update
